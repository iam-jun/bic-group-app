{
  "name": "rn_bein",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clone-build-resources": "rm -rf bic-mobile-build-resources && git clone git@github.com:beincom/bic-mobile-build-resources.git && rm -rf bic-mobile-build-resources/.git",
    "clone-secret": "rm -rf bic-mobile-secret && git clone git@github.com:beincom/bic-mobile-secret.git && rm -rf bic-mobile-secret/.git",
    "clone-prepare-resources": "npm run clone-build-resources && npm run clone-secret && npm run prepare-resources",
    "prepare-resources": "./scripts/prepare-resources.sh",
    "build-android-staging": "./scripts/build.sh apk staging",
    "build-android-prerelease": "./scripts/build.sh apk prerelease",
    "build-android-production": "./scripts/build.sh apk production",
    "build-ios-staging": "./scripts/build.sh ipa staging",
    "build-ios-prerelease": "./scripts/build.sh ipa prerelease",
    "build-ios-production": "./scripts/build.sh ipa production",
    "bundle-aab": "./scripts/build.sh aab",
    "clean": "./scripts/clean.sh",
    "codepush-ios": "appcenter codepush release-react -a Beincom/BIC-Group-iOS -d Staging --plist-file=\"ios/bic_group/Info.plist\"",
    "codepush-android": "appcenter codepush release-react -a Beincom/BIC-Group -d Staging",
    "postinstall": "patch-package && husky install",
    "precommit": "npm run lint && npm run tsc",
    "lint": "npx eslint --fix .",
    "tsc": "NODE_OPTIONS=--max_old_space_size=12000 tsc",
    "prettier-format": "npx prettier --config .prettierrc.json \"**/*.{js,jsx,ts,tsx}\" --write",
    "start": "npx react-native start",
    "start:clean": "watchman watch-del '/Users/namanh/ReactNative/bein-comm' ; watchman watch-project '/Users/namanh/ReactNative/bein-comm' && npm start -- --reset-cache",
    "android": "npx react-native run-android --variant=developmentDebug",
    "android-release": "npx react-native run-android --variant=developmentRelease",
    "android-staging": "npx react-native run-android --variant=stagingDebug",
    "android-staging-release": "npm run android -- --variant=stagingRelease",
    "ios": "npx react-native run-ios --scheme=bic_group_dev",
    "ios-release": "npx react-native run-ios --scheme=bic_group_dev --configuration=ReleaseDevelopment",
    "ios-staging": "npx react-native run-ios --scheme=bic_group_staging --configuration=DebugStaging",
    "ios-staging-release": "npx react-native run-ios --scheme=bic_group_staging --configuration=ReleaseStaging",
    "start-android": "adb reverse tcp:8081 tcp:8081 && react-native start --port=8081",
    "test android dev": "detox src/test --configuration android.dev.debug",
    "test android dev-release": "detox src/test --configuration android.dev.release",
    "sync-profile": "fastlane ios sync_profile",
    "deploy-staging-ios": "fastlane ios deploy_staging",
    "deploy-production-ios": "fastlane ios deploy_production",
    "build-staging-android": "fastlane android build_staging",
    "build-production-android": "fastlane android build_production",
    "deploy-staging-android": "fastlane android deploy_staging",
    "deploy-production-android": "fastlane android deploy_production",
    "test": "detox",
    "jest": "jest",
    "jest:cov": "jest --coverage --silent --testResultsProcessor=jest-sonar-reporter",
    "jest:ci": "NODE_OPTIONS=--max-old-space-size=1024 jest --maxWorker=50% --coverage --testResultsProcessor=jest-sonar-reporter",
    "jest:open-cov": "open ./coverage/lcov-report/index.html",
    "reverse": "adb reverse tcp:8081 tcp:8081 && adb reverse tcp:9090 tcp:9090",
    "update-stories": "sb-rn-get-stories --config-path .ondevice",
    "prestart": "npm run update-stories",
    "storybook-watcher": "sb-rn-watcher --config-path .ondevice",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
